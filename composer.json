{
    "name": "11ya/actual-user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to refresh user roles after its changing",
    "keywords": ["security", "user", "user-roles", "authorization"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ilya Vertakov",
            "email": "ilya.saratov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1|~3.0",
        "symfony/security": "~2.1|~3.0"
    },
    "autoload": {
        "psr-4": { "Ilya\\ActualUserBundle\\": "" }
    },
    "suggest": {
        "friendsofsymfony/user-bundle": "Easy to use and override default provider and entity classes",
        "doctrine/doctrine-bundle": "Using with Doctrine ORM",
        "doctrine/mongodb-odm-bundle": "Using with Doctrine MongoDB ODM"
    }
}
